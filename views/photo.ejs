<!DOCTYPE html>
<html lang="en">
<%- include('./partials/_head') %>

   <body class="main-layout inner_page">
      <%- include('./partials/_header') %>
         <div class="about">
            <div class="container">
               <div class="row">
                  <div class="col-md-6">
                     <div class="titlepage text_align_left">
                        <h2>
                           <%= photo.name %>
                        </h2>
                        <p>
                           <%= photo.description %>
                        </p>
                        <% if(JSON.stringify(photo.user._id)==JSON.stringify(exist_user) ){ link='/users/dashboard'
                           }else { link='/users/' +photo.user._id } %>

                           <a href="<%= link %>" class="read_more">
                              <%= photo.user.username %>
                           </a>
                     </div>
                  </div>
                  <div class="col-md-6">
                     <div class="about_img">
                        <figure><img src="<%= photo.url %>" alt="#" /></figure>
                     </div>
                  </div>
               </div>
            </div>
         </div>

         <%- include('./partials/_footer') %>
            <%- include('./partials/_script') %>
   </body>

</html>